                            "target": "https://nationalgalleryofart.github.io/accademia-maps-iiif/munsterromemapjacobs/canvas/1#xywh=6874,1513,758,1022",
                            "body": {
                                "type": "TextualBody",
                                "format": "text/html",
                                "value": "<p>Vatican (Apostolic) Palace (legend B: Des Bapsts Pallast; labels: Palatium Papae, Bel Videre)</p>"
                            }
